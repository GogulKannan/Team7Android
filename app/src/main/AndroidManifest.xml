<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gogul.adandroid">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/team7"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:windowSoftInputMode="adjustPan">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Store"
            android:label="@string/title_activity_store"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Department"
            android:label="@string/title_activity_department"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RetrievalAll"
            android:label="@string/title_activity_retrieval_all"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DisbursementList"
            android:label="@string/title_activity_disbursement_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DisbursementMainmenu"
            android:label="@string/title_activity_disbursement_mainmenu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ViewRequisition"
            android:label="@string/title_activity_view_requisition"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ApproveRequisition"
            android:label="@string/title_activity_approve_requisition"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ChangeCollection"
            android:label="@string/title_activity_change_collection"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ReceiveRequisition"
            android:label="@string/title_activity_receive_requisition"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Stockdetails"
            android:label="@string/title_activity_stockdetails"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".StockCard"
            android:label="@string/title_activity_stock_card"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".StockSupplier"
            android:label="@string/title_activity_stock_supplier"
            android:theme="@style/AppTheme.NoActionBar" />
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".ViewRequistionOne"
            android:label="@string/title_activity_view_requistion_one"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ApproveOneReq"
            android:label="@string/title_activity_approve_one_req"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ViewCollectionDetails"
            android:label="@string/title_activity_view_collection_details"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DeliveryAdj"
            android:label="@string/title_activity_delivery_adj"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".UnfulfilledRequisitions"
            android:label="@string/title_activity_unfulfilled_requisitions"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>

        <service android:name=".MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>
    </application>

</manifest>